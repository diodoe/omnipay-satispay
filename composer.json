    "description": "Satispay driver for the Omnipay payment processing library",
    "keywords": [
        "satispay",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/diodoe/omnipay-satispay",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabio Giannese",
            "email": "me@diodoe.com"
        },
        {
            "name": "Germano Zambelli",
            "homepage": "https://github.com/GermanoZambelli"
        },
        {
            "name": "Massimo Chiarillo",
            "homepage": "https://github.com/ChiarilloMassimo"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\Satispay\\" : "src/" }
    },
    "require": {
        "omnipay/common": "~3.0",
        "friendsofphp/php-cs-fixer": "^2.13"
